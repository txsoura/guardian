    "type": "library",
    "keywords": [
        "txsoura",
        "core",
        "laravel"
    ],
    "homepage": "https://github.com/txsoura/core",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Tesoura JÃºnior",
